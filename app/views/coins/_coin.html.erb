<div id="<%= dom_id coin %>">
  <p>
    <strong>Descrição:</strong>
    <%= coin.description %>
  </p>

  <p>
    <strong>Sigla:</strong>
    <%= coin.acronym %>
  </p>

  <p>
    <strong>Url da Imagem:</strong>
    <%= image_tag coin.url_image, width: 20, height: 20 %>
  </p>

   <p>
    <strong>Tipo de Mineração:</strong>
    <%= coin.mining_type.description %>
  </p>

</div>
